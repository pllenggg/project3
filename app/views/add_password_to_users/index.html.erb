<p id="notice"><%= notice %></p>

<h1>Add Password To Users</h1>

<table>
  <thead>
    <tr>
      <th>Password digest</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_password_to_users.each do |add_password_to_user| %>
      <tr>
        <td><%= add_password_to_user.password_digest %></td>
        <td><%= add_password_to_user.string %></td>
        <td><%= link_to 'Show', add_password_to_user %></td>
        <td><%= link_to 'Edit', edit_add_password_to_user_path(add_password_to_user) %></td>
        <td><%= link_to 'Destroy', add_password_to_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add Password To User', new_add_password_to_user_path %>
